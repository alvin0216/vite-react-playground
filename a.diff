+const exec = require("child_process").exec;
+
+function cmd(cmdStr) {
+  return new Promise((resolve, reject) => {
+    exec(cmdStr, function (error, stdout, stderr) {
+      if (stderr) {
+        console.log("cmd error: ", stderr);
+        reject(stderr);
+      } else resolve(stdout);
+    });
+  });
+}
+
+const projectPath = "/Users/guoshaowei/Desktop/code/vantage-dev-tools2";
+
+async function fn() {
+  // await cmd("git pull");
+  const stdout1 = await cmd(`cd ${projectPath} && git tag --sort=creatordate`);
+  console.log(stdout1);
+}
+
+fn();
